*
	margin 0
	padding 0
	outline none
body
	font-size: 18px
	font-family: OS
	background-color #fff
	color #000
	line-height: 1.6
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display block
	text-align center
	margin 0 auto
	color: #FFFFFF;
	font-size: 18px
	font-family: R
	width: 100%
	max-width: 350px;
	padding 18px 10px
	background-color #5C145D;
	box-shadow: 0px 4px 50px rgba(92, 20, 93, 0.24);
	border-radius: 100px;
	text-decoration: none
	transition: all .3s ease
	&:focus
		color #fff
		text-decoration: none
	&:hover
		text-decoration: none
		color #fff
		background-color #5C145D - 20%
		transform: translateY(2px)
		box-shadow: 0px 4px 25px rgba(92, 20, 93, 0.44);
.general-title
	text-align center
	color #000
	font-size: 30px
	font-family: RS
	margin-bottom: 50px